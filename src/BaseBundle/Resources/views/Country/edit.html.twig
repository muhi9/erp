{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}


{% block title %}{{'country.country'|trans }}{% endblock %}

{% block content_body %}

<div class="row">
  <div class="col-md-12">
    <div class="k-portlet">
      <div class="k-portlet__head">
        <div class="k-portlet__head-label">
        <a href="{{ path('country_list', { 'page': 'country_edit'})}}" class="k-margin-r-5  btn btn-brand k-content__head-back-link"><i class="flaticon-reply"></i>{{'label.back'|trans }}</a>
 
          <h3 class="k-portlet__head-label">{{'country.country_edit'|trans({'%country%':form.vars.data|default()}) }}</h3>
        </div>
      </div>
      {{ form_start(form,{attr: {novalidate:'novalidate'}})}}
      {{ form_widget(form) }}
      {{ form_end(form) }}
    </div>
  </div>
</div>

{% endblock content_body %}
{% block content_breadcrumb %}
  <a href="{{ path('homepage')}}" class="k-content__head-breadcrumb-link">{{'label.dashboards'|trans }}</a>
  <span class="k-content__head-breadcrumb-separator"></span>
  <a href="{{ path('country_list', {'page': 'country_edit'})}}" class="k-content__head-breadcrumb-link">{{'country.country_list'|trans }}</a>
  <span class="k-content__head-breadcrumb-separator"></span>
  <span class="k-content__head-breadcrumb-link k-content__head-breadcrumb-link--active">{{'country.country_breadcrumb_edit'|trans }}</span>    
{% endblock content_breadcrumb %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}
