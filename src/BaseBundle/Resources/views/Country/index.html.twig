{% extends 'base.html.twig' %}
{% import 'base_datatable.html.twig' as datatable %}
{% block title %}{{'country.country_list'|trans }}{% endblock %}

{% block content_body %}


<!-- begin:: Content Body -->
<div class="modal fide" role="dialog"></div>
<div class="k-content__body    k-grid__item k-grid__item--fluid" id="k_content_body">
    <div class="k-portlet k-portlet--mobile">
        <div class="k-portlet__head">
            <div class="k-portlet__head-label">
                <h3 class="k-portlet__head-title">{{'country.country_list'|trans }}</h3> &nbsp;
            </div>
            <div class="k-portlet__head-label">
            <a href="{{path('country_add')}}" class="btn btn-sm k-margin-l-5 btn-elevate btn-brand">
                <i class="flaticon2-add-1  k-padding-l-5 k-padding-r-0"></i>
                <span class="k-font-bold">{{'label.addNew'|trans}}</span>&nbsp;
            </a>
            </div>
        </div>
        <div class="k-portlet__body main">
            {{ datatable.html_block(table) }}
        </div>
    </div>
</div>

<!-- end:: Content Body -->


{% endblock content_body %}



{% block javascripts %}
    {{ parent() }}
    {{ datatable.script_block(table) }}
{% endblock %}
