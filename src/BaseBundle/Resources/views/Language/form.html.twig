{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}


{% block title %}{{'title.language'|trans }}{% endblock %}

{% block content_body %}

<div class="row">
  <div class="col-md-12">
    <div class="k-portlet">
      <div class="k-portlet__head">
        <div class="k-portlet__head-label">
          <h3 class="k-portlet__head-label">{{headerTitle|trans({'%language%':form.vars.data|default()}) }}</h3>
        </div>
        <div class="k-portlet__head-label">
          <a href="{{ path('language_list', {'page': 'language_add'})}}" class="k-margin-r-5  btn btn-brand k-content__head-back-link"><i class="flaticon-reply"></i>{{'label.back'|trans }}</a>
        </div>

      </div>
      {{ form_start(form,{attr: {novalidate:'novalidate'}})}}
      {{ form_widget(form) }}
      {{ form_end(form) }}
    </div>
  </div>
</div>

{% endblock content_body %}


{% block javascripts %}
    {{ parent() }}
{% endblock %}
