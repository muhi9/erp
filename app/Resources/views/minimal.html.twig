    '@BaseBundle/Resources/public/plugins/bootstrap/datepicker.min.js'
    '@BaseBundle/Resources/public/plugins/bootstrap/timepicker.js'
    '@BaseBundle/Resources/public/plugins/bootstrap/touchspin.js'

