<script>{% spaceless %}
    {% set urls = {} %}
    {% if is_granted('ROLE_ADMIN') %}
        {% for url in javascript_urls.admin%}
            {% set urls = urls|merge({(url):path(url)}) %}
        {%endfor%}
    {% else %}
        {% for url in javascript_urls.user%}
            {% set urls = urls|merge({(url):path(url)}) %}
        {%endfor%}
    {% endif %}
    {% for url in javascript_urls.all%}
        {% set urls = urls|merge({(url):path(url)}) %}
    {%endfor%}

    var urls = {{urls|json_encode|raw}};
    var env = '{{app.environment}}';
    var profileId = {{app.user.info.id}};
{% endspaceless %}
</script>

